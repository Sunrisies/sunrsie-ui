{
    "name": "sunrise-ui",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "turbo dev",
        "clean": "turbo clean",
        "lint": "pnpm run --recursive lint",
        "test": "pnpm run --recursive test",
        "docs:dev": "pnpm --filter sunrise-ui-docs dev",
        "docs:build": "pnpm --filter sunrise-ui-docs build",
        "docs:preview": "pnpm --filter sunrise-ui-docs preview"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "rimraf": "^5.0.5",
        "turbo": "2.6.0",
        "typescript": "^5.0.0"
    },
    "packageManager": "pnpm@10.0.0"
}